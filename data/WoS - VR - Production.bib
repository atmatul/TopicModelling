@article{ ISI:A1995RG44100003,
Author = {OKI, K and OZAWA, L},
Title = {{A 0.6IN CRT AS A MINIATURE DISPLAY DEVICE}},
Journal = {{DISPLAYS}},
Year = {{1995}},
Volume = {{16}},
Number = {{2}},
Pages = {{59-67}},
Month = {{APR}},
Abstract = {{This paper discusses the miniaturization of display devices that are
   capable of displaying information from VGA and SVGA as well as video
   movies and TV images. We conclude that miniature CRTs are suitable
   display devices for mounting on helmets, headbands and spectacles. We
   then developed the 0.6in miniature CRT based on new concepts in the
   design of the electron gun and the phosphor screen. The miniature CRT
   developed has high resolution (800 x 600), high brightness (2000
   cd/m(2)), low power consumption (1.8 W for NTSC operation), is
   lightweight (6 g), and has low production cost. These 0.6in miniature
   CRTs can now be considered for new applications.}},
DOI = {{10.1016/0141-9382(95)91175-2}},
ISSN = {{0141-9382}},
Unique-ID = {{ISI:A1995RG44100003}},
}

@article{ ISI:A1995RA95400007,
Author = {DOU, H and DOU, JM},
Title = {{VIRTUAL RESEARCH UNIVERSES AND RESEARCH MANAGEMENT}},
Journal = {{TECHNOLOGY ANALYSIS \& STRATEGIC MANAGEMENT}},
Year = {{1995}},
Volume = {{7}},
Number = {{1}},
Pages = {{109-122}},
Abstract = {{The analysis of large research production, such as universities, towns,
   cities or event countries, can be performed in several ways. These
   various approaches can range from the experts' feeling to a more precise
   analysis of co-authors' productivity. The various results obtained
   introduce the concept of `virtual research universes', where the global
   perception of the production hides the sharp reality of objectives and
   subjects. The ability to apply the various methodologies and tools to
   make such analysis can reduce misunderstanding and misleading positions,
   especially in funding, planning and project evaluation. This paper takes
   as an example the production of physics laboratories in Marseille (from
   the INSPEC database), and shows how the various levels of analysis can
   move from virtual to real research universes, thus helping the experts
   in their decision process.}},
DOI = {{10.1080/09537329508524198}},
ISSN = {{0953-7325}},
Unique-ID = {{ISI:A1995RA95400007}},
}

@article{ ISI:A1994PP94900007,
Author = {BUNKER, LK},
Title = {{VIRTUAL-REALITY - MOVEMENTS CENTRALITY}},
Journal = {{QUEST}},
Year = {{1994}},
Volume = {{46}},
Number = {{4}},
Pages = {{456-474}},
Month = {{NOV}},
Abstract = {{The profession of physical education must refocus on the centrality of
   the study of human movement.  Physical educators have too long diluted
   the basic field rather than emphasize the common mission of
   understanding human movement and the contribution it makes to human
   physical and mental health.  Ten principles are set fourth to aid in
   `'Reclaiming and Transforming Physical Education'':  (a) Emphasize the
   uniqueness of human movement; (b) put first things first in establishing
   a common core of knowledge for all professionals; (c) appreciate the
   `'web of connectedness''; (d) enhance communication between and among
   professionals; (e) take control and responsibility for ethical action,
   (f) act on principles; (g) appreciate diversity; (h) uphold standards of
   fairness; (i) facilitate the collective production of practical and
   theoretical knowledge; and (j) transform physical education by
   anticipating and creating change.}},
DOI = {{10.1080/00336297.1994.10484139}},
ISSN = {{0033-6297}},
Unique-ID = {{ISI:A1994PP94900007}},
}

@article{ ISI:A1994PR85600006,
Author = {IMAMURA, K and NOMURA, J},
Title = {{VIRTUAL SPACE DECISION-SUPPORT SYSTEM}},
Journal = {{ADVANCED ROBOTICS}},
Year = {{1994}},
Volume = {{8}},
Number = {{4}},
Pages = {{413-426}},
Abstract = {{A recent manifestation of the new paradigm regarding the relationship
   between humans and computers is called artificial reality, virtual
   reality or cyberspace.  Using 3D computer graphics, multisensory
   interactive interface devices and high resolution displays, a virtual
   world can be realized in which one can pick up imaginary objects as if
   they are real.  Using this technology, the virtual space decision
   support system (VSDSS) lets users design virtual products and experience
   them while in virtual space.  The VSDSS and its application to the
   unified sales/production system (US/PS) are described in this paper.  At
   present, most computer-aided manufacturing systems are geared towards
   production in small lots and are unable to handle custom-designed
   products.  Using the VSDSS, individuals will be able to design their own
   products.  The VSDSS system should be designed so that the dreams of the
   individuals can be realized.}},
ISSN = {{0169-1864}},
Unique-ID = {{ISI:A1994PR85600006}},
}

@article{ ISI:A1994QA92200001,
Author = {MCEWAN, JA},
Title = {{REAL-TIME 3-D - THE SOUND OF THE TIMES}},
Journal = {{GEC REVIEW}},
Year = {{1994}},
Volume = {{9}},
Number = {{2}},
Pages = {{67-80}},
Abstract = {{Over the years, emphasis has been put on the production of visuals to
   the point of near perfection. However, as the peak of the exponential
   curve oi visual perfection is reached, how are other human senses
   shaping up to simulation? This paper addresses `audio simulation',
   examining the concepts and properties which allow it to enhance visuals
   for only small investment.}},
ISSN = {{0267-9337}},
Unique-ID = {{ISI:A1994QA92200001}},
}

@article{ ISI:A1993LU77500003,
Author = {JONES, S},
Title = {{A SENSE OF SPACE - VIRTUAL-REALITY, AUTHENTICITY AND THE AURAL}},
Journal = {{CRITICAL STUDIES IN MASS COMMUNICATION}},
Year = {{1993}},
Volume = {{10}},
Number = {{3}},
Pages = {{238-252}},
Month = {{SEP}},
Abstract = {{This paper re-examines the history of the apprehension, consumption and
   production of reality by means of the aural in light of developments in
   virtual reality technology. It argues thal virtual reality has long been
   a domain of audio technology and that by acknowledging and examining its
   roots in aural media its connections to the discourse of authenticity
   are made clear. It proposes that critical analysis of virtual reality
   must make explicit its undeclared nature as a mediated and symbolic
   creation.}},
DOI = {{10.1080/15295039309366866}},
ISSN = {{0739-3180}},
Unique-ID = {{ISI:A1993LU77500003}},
}

