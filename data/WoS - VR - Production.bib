@inproceedings{ ISI:000389274300104,
Author = {Singh, Deepanshu R. and Mishra, Arvind K.},
Editor = {{Kumar, C and Banka, H and Ramesh, D}},
Title = {{Review of IT Enabled Technologies in Indian Mining Industry for Improved
   Productivity \& Safety}},
Booktitle = {{2016 3rd International Conference on Recent Advances in Information
   Technology (RAIT)}},
