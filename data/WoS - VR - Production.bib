}

@inproceedings{ ISI:000265675400069,
Author = {Tumeo, Antonino and Branca, Marco and Camerini, Lorenzo and Ceriani,
   Marco and Monchiero, Matteo and Palermo, Gianluca and Ferrandi, Fabrizio
   and Sciuto, Donatella},
Book-Group-Author = {{IEEE}},
Title = {{Prototyping Pipelined Applications on a Heterogeneous FPGA
   Multiprocessor Virtual Platform}},
Booktitle = {{PROCEEDINGS OF THE ASP-DAC 2009: ASIA AND SOUTH PACIFIC DESIGN
   AUTOMATION CONFERENCE 2009}},
Series = {{Asia and South Pacific Design Automation Conference Proceedings}},
Year = {{2009}},
