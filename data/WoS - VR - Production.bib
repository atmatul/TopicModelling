@inproceedings{ ISI:000427148600005,
Author = {Bouchard, Jonathan and Clark, James J.},
Book-Group-Author = {{IEEE}},
Title = {{HALF-OCCLUDED REGIONS: THE KEY TO DETECTING A DIVERSE ARRAY OF DEFECTS
   IN S3D IMAGERY}},
Booktitle = {{2017 INTERNATIONAL CONFERENCE ON 3D IMMERSION (IC3D)}},
Year = {{2017}},
Note = {{7th International Conference on 3D Immersion (IC3D), Brussels, BELGIUM,
   DEC 11-12, 2017}},
Organization = {{Inst Elect \& Elect Engineers Signal Proc Soc}},
Abstract = {{Half-occlusions, the regions of a binocular image which are only visible
   in one view, convey important depth ordering information that is left
   aside by most stereo computer vision algorithms and stereo
   post-production practices of the stereoscopic 3D (S3D) film industry. We
   demonstrate the efficacy of using rules governing the geometry of
   half-occlusions to detect occlusion-stereopsis conflicts in S3D imagery.
   Experimentation shows that our half-occlusion based technique allow high
   accuracy detection of global and local pseudoscopy, stereo window
   violations and rotoscopy mismatches at a low computational cost for both
   stereo matched and synthetic disparity maps. Such a detection algorithm
   could prove useful in quality control of S3D imagery for cinema and
   virtual reality.}},
ISBN = {{978-1-5386-4655-7}},
Unique-ID = {{ISI:000427148600005}},
}

@inproceedings{ ISI:000427148600017,
Author = {Croci, Simone and Knorr, Sebastian and Goldmann, Lutz and Smolic, Aljosa},
Book-Group-Author = {{IEEE}},
Title = {{A FRAMEWORK FOR QUALITY CONTROL IN CINEMATIC VR BASED ON VORONOI PATCHES
   AND SALIENCY}},
Booktitle = {{2017 INTERNATIONAL CONFERENCE ON 3D IMMERSION (IC3D)}},
Year = {{2017}},
Note = {{7th International Conference on 3D Immersion (IC3D), Brussels, BELGIUM,
   DEC 11-12, 2017}},
Organization = {{Inst Elect \& Elect Engineers Signal Proc Soc}},
Abstract = {{In this paper, we present a novel framework for quality control in
   cinematic VR (360-video) based on Voronoi patches and saliency which can
   be used in post-production workflows. Our approach first extracts
   patches in stereoscopic omnidirectional images (ODI) using the spherical
   Voronoi diagram. The subdivision of the ODI into patches allows an
   accurate detection and localization of regions with artifacts. Further,
   we introduce saliency in order to weight detected artifacts according to
   the visual attention of end-users. Then, we propose different artifact
   detection and analysis methods for sharpness mismatch detection (SMD),
   color mismatch detection (CMD) and disparity distribution analysis. In
   particular, we took two state of the art approaches for SMD and CMD,
   which were originally developed for conventional planar images, and
   extended them to stereoscopic ODIs. Finally, we evaluated the
   performance of our framework with a dataset of 18 ODIs for which
   saliency maps were obtained from a subjective test with 17 participants.}},
ISBN = {{978-1-5386-4655-7}},
Unique-ID = {{ISI:000427148600017}},
}

