@inproceedings{ ISI:000326477900022,
Author = {Menck, Nicole and Weidig, Christian and Aurich, Jan C.},
Editor = {{Cunha, PF}},
Title = {{Virtual Reality as a Collaboration Tool for Factory Planning based on
   Scenario Technique}},
Booktitle = {{FORTY SIXTH CIRP CONFERENCE ON MANUFACTURING SYSTEMS 2013}},
