@inproceedings{ ISI:000262624900007,
Author = {Su, Chuan-Jun and Chuang, Shin-Chi},
Book-Group-Author = {{Chinese Univ Hong Kong}},
Title = {{Virtual Reality based Product Configuration for Mass Customization in
   e-Business}},
Booktitle = {{PROCEEDINGS OF THE SECOND INTERNATIONAL CONFERENCE ON OPERATIONS AND
   SUPPLY CHAIN MANAGEMENT}},
Year = {{2008}},
Pages = {{45-50}},
Note = {{2nd International Conference on Operations and Supply Chain Management,
   Taiwan Acad Banking \& Finance, Taipei, TAIWAN, JUL 29-AUG 02, 2008}},
Abstract = {{The infusion of technologies such as Internet, Virtual Reality
   (VR)/Animation, Enterprise Resource Planning (ERP) into business
   operations is drastically evolving the way businesses operate. Customers
   gain more bargaining power than ever through the ubiquity of the
   Internet. Customers demand products with lower prices, higher quality
   and speedy delivery, but they also want products customized to match
   their unique requirement. To meet these demands, companies have to
   evolve their business model from traditional mass production to mass
   customization. Mass customization allows customers to select order and
   receive a specially configured product, often choosing from among
   hundreds of product options, at a low cost, and short lead-time of
   delivery over the Internet. To fully support mass customization in
   e-business, a proposed system that equips with a VR-based product
   configuration module to allow customers specify unique requirements and
   visualizes the resulting product in 3D and an ERP backend support engine
   to provide real-time information on cost and delivery lead-time is
   proposed in this research.}},
Unique-ID = {{ISI:000262624900007}},
}

