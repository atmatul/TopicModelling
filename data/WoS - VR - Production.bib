Unique-ID = {{ISI:000174698500016}},
}

@article{ ISI:000174812800003,
Author = {Kramer, P},
Title = {{Target-oriented management of forest production processes - dream or
   reality?}},
Journal = {{ALLGEMEINE FORST UND JAGDZEITUNG}},
Year = {{2002}},
Volume = {{173}},
Number = {{2-3}},
Pages = {{37-47}},
Month = {{FEB-MAR}},
Abstract = {{Title of the paper: Target-oriented management of forest production
   processes - dream or reality?
   The long-term duration of forest production processes influences actions
   in forestry enterprises to a great extent and complicates the concrete
   time frame of specific goals and actions. According to this fact
   forestry tries to manage long-term production processes with the help of
   a specific forest planning. However, because of the long-term duration
   of forestry production processes you can find different opinions in the
   forestry discussion if target-oriented actions are possible or not.
   The aim of this research project is to analyse how long-term strategic
   goals are implemented into forestry action under specific forestry
   conditions such as long time horizons and complexity. The study
   participants are observed and their actions are analysed using a virtual
   computer-simulated stand modeled on real stand data. To visualize growth
   and exploitation a tree growth simulator is used. The state of the stand
   can be visualized using a three-dimentional graphic. With the help of
   this computer simulator the factor time can be controlled and minimized.
   The experiment offers the possibility of a timeless observation of
   forestry action for the entire duration of the stand.
   The results of the study show extrem differences of individual stand
   production processes and thereby question the possibility of
   target-oriented management of processes in forestry enterprises, It is
   supposed that the individual skills play a major role rather than the
   quality of operational goal formulation.}},
ISSN = {{0002-5852}},
Unique-ID = {{ISI:000174812800003}},
}

@inproceedings{ ISI:000175618000013,
Author = {Knudsen, CJS and Naeve, A},
Editor = {{Soldek, J and Pejas, J}},
Title = {{Presence production in a distributed shared virtual environment for
   exploring mathematics}},
Booktitle = {{ADVANCED COMPUTER SYSTEMS, PROCEEDINGS}},
Series = {{KLUWER INTERNATIONAL SERIES IN ENGINEERING AND COMPUTER SCIENCE}},
Year = {{2002}},
Volume = {{664}},
Pages = {{149-159}},
Note = {{8th International Conference on Advanced Computer Systems (ACS 2001),
   MIELNO, POLAND, OCT 17-19, 2001}},
Abstract = {{It is well known that the current state of mathematics education is
   problematic in many countries. The Interactive Learning Environments
   group at CID (Centre for user-oriented IT Design) at the Royal Institute
   of Technology (KTH) has developed an avatar-based shared virtual
   environment called CyberMath, aimed at improving this situation by
   allowing interaction with mathematical content in new and exciting ways.
   CyberMath is suitable for exploring and teaching mathematics in
   situations where both the teacher and the students are co-present and
   physically separated. In this virtual reality environment the
   participants are represented by avatars. The space concept in virtual
   environments is different from that of any known physical space. Yet
   people seem to perceive, for example, chat rooms and bulletin board
   systems as places. Still, avatars have limited possibilities for
   non-verbal expressions, such as body language, which are important in
   order to improve the communication quality. To investigate the
   importance of human-to-human expression and eye-contact between actors
   in the CyberMath environment, a test lecture in mathematics was carried
   out between students at Uppsala University and a lecturer at the Royal
   Institute of Technology (KTH) in Stockholm. The Media Environment group
   at the KTH Learning Lab was responsible for the production of a sense of
   presence involving the lecturer and the students, using distance
   technology such as networked two-way television systems and interactive
   storytelling. Empirical material was collected from recordings of 5
   video sources and through a questionnaire given to the participants in
   the test. The main goal of the study was to investigate whether students
   at a distance could adapt to a combination of different shared virtual
   environments. It was found that presence production mediated as two-way
   television is a good way to build trust and to enhance non-verbal
   communication between the actors. The students treated the avatars on
   the computer screen and the lecturer on the display in front of them as
   real persons. In the same way, they treated the virtual reality space
   and the space distributed through two-way television as real spaces.}},
ISBN = {{0-7923-7651-X}},
Unique-ID = {{ISI:000175618000013}},
}

@inproceedings{ ISI:000177688000008,
Author = {Liang, G and Wang, C and Mi, W and Huang, Y},
Editor = {{Shahin, TMM}},
Title = {{Development of training simulators}},
Booktitle = {{COMPUTER-BASED DESIGN}},
Year = {{2002}},
Pages = {{109-129}},
Note = {{Engineering Design Conference 2002, KINGS COLL, LONDON, ENGLAND, JUL
   09-11, 2002}},
Abstract = {{In the past, the traditional training method of `hand to hand' teaching
   was adopted in industry. This method needs a long time and its
   efficiency is low, it is also affect the production and cannot keep up
   with the renewal of devices and equipments. Moreover may do harm to a
   practical system or devices and equipment. So it is necessary to have a
   new training method to carry out operation training. This is just the
   training simulator. In this paper, the introduction of training
   simulator about what is training simulator, why we use it, how it is
   done is presented. Moreover the container crane training simulator based
   on virtual reality and programmable logic control system (PLC) is
   introduced. In addition, the functions, working theory, composition and
   communication structure of the container crane training simulator are
   presented and some conclusions are obtained.}},
ISBN = {{1-86058-372-5}},
Unique-ID = {{ISI:000177688000008}},
}

@inproceedings{ ISI:000180731100016,
Author = {Schafer, K and Bruns, W},
Editor = {{Bernhardt, R and Erbe, HH}},
Title = {{PLC-programming by demonstration using graspable models}},
Booktitle = {{COST ORIENTED AUTOMATION - (LOW COST AUTOMATION 2001)}},
Series = {{IFAC SYMPOSIA SERIES}},
Year = {{2002}},
Pages = {{101-106}},
Note = {{6th IFAC Symposium on Cost Oriented Automation, BERLIN, GERMANY, OCT
   08-09, 2001}},
Organization = {{Int Federat Automat Control}},
Abstract = {{PLC-programming for production plants is time and money consuming. It
   takes place in the critical period shortly before start of production
   which is a very time critical period. It should be started as early as
   possible, using concurrent engineering, interdisciplinary cooperation,
   efficient programming tools and simulation. Programming by demonstration
   using graspable models brings these techniques together and represents a
   crucial new approach. Graspable models of solid bricks are well suited
   for factory planning. Using these graspable models also for programming
   may improve cooperation and understanding of interdisciplinary experts.
   The Research Center artec features a new concept called Real Reality to
   synchronize real and virtual worlds dynamically. A magnetic tracking
   system serves as input device together with a data glove or a sensor
   ring. These devices provide input data that allow to keep track of the
   changes of the real world model. An automatic system for input
   abstraction generates PLC programs automatically from demonstrated
   behavior. The benefits of this concept are shorter time to production,
   lower costs, better control program validity and better cooperation in
   interdisciplinary teams. Copyright (C) 2001 IFAC.}},
ISSN = {{0962-9505}},
ISBN = {{0-08-043907-1}},
Unique-ID = {{ISI:000180731100016}},
}

@inproceedings{ ISI:000179334800036,
Author = {Terazawa, T and Osaki, H and Kajihara, Y and Munesawa, Y},
Editor = {{Xia, GP and Osaki, H}},
Title = {{Assembly process design system using Virtual Reality}},
Booktitle = {{ICIM' 2002: PROCEEDINGS OF THE SIXTH CHINA-JAPAN INTERNATIONAL
