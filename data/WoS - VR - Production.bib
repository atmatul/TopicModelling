Title = {{Design and fabrication of concave-convex lens for head mounted virtual
   reality 3D glasses}},
Booktitle = {{2015 INTERNATIONAL CONFERENCE ON OPTICAL INSTRUMENTS AND TECHNOLOGY:
   OPTOELECTRONIC MEASUREMENT TECHNOLOGY AND SYSTEMS}},
Series = {{Proceedings of SPIE}},
