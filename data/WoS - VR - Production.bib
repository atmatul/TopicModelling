@inproceedings{ ISI:000176421500009,
Author = {Grladinovic, T},
Editor = {{Despot, R}},
Title = {{Production networks in wood processing and furniture manufacturing -
   Support to revival and reconstruction}},
Booktitle = {{WOOD IN CONSTRUCTION INDUSTRY - PROSPECTIVES OF RECONSTRUCTION}},
Year = {{2002}},
Pages = {{75-82}},
Note = {{International Conference on Wood in Construction Industry, ZAGREB,
   CROATIA, APR   19, 2002}},
Organization = {{Minist Public Works, Reconstruct \& Construct; Minist Sci \& Technol;
   Minist Environm Protect \& Spatial Planning; Croatian Econ Chamber;
   Hrvatske Sume po; Inst Forestry Jastrebarsko; Croatian Forestry Soc; Res
   Inst Wood Technol; Univ Zagreb, Fac Forestry; Zagreb Fair}},
Abstract = {{Non-flexible production structure can no longer survive on a turbulent
   market, since it can react to changes only with a major lag. In order to
   satisfy specific customer needs related to performance, quality,
   quantity, cost and delivery terms, it is necessary to restructure the
   existing factories, which are still organised according to the Taylor
   principle. One of the reconstruction possibilities is to implement new
   co-operation forms leading to virtual factories. For a virtual factory
   to be efficient, it needs to utilise modern communication-information
   technology. This paper presents and analyses the basic
   communication-information technologies that can be used to connect the
   participants within the virtual factory. The factory of the future will
   permanently develop. Its contours and structures will not be clearly
   recognised as those of the traditional factory, but will have the
   characteristics of virtual reality. New business relationships will be
   stronger and longer, just like the link with suppliers and customers.
   This requires a permanent, detailed restructuring and reorganisation of
   the enterprise's network and new sophisticated communication-information
   technologies. That is the future for many Croatian furniture
   manufacturing and wood products. If the idea is realised, the real
   virtual factory will influence the furniture market, as well as costs of
   furniture and wood products. Our firms would be more competitive and
   profitable.}},
ISBN = {{953-6307-58-8}},
Unique-ID = {{ISI:000176421500009}},
}

@article{ ISI:000172711300008,
Author = {Lehmann, FO},
Title = {{The efficiency of aerodynamic force production in Drosophila}},
Journal = {{COMPARATIVE BIOCHEMISTRY AND PHYSIOLOGY A-MOLECULAR AND INTEGRATIVE
   PHYSIOLOGY}},
Year = {{2001}},
Volume = {{131}},
Number = {{1, SI}},
Pages = {{77-88}},
Month = {{DEC}},
Note = {{European-Society-for-Comparative-Physiology-and-Biochemistry Congress,
   LIEGE, BELGIUM, JUL 24-28, 2000}},
Organization = {{European Soc Comparat Physiol \& Biochem}},
Abstract = {{Total efficiency of aerodynamic force production in insect flight
   depends on both the efficiency with which flight muscles turn metabolic
   energy into muscle mechanical power and the efficiency with which this
   power is converted into aerodynamic flight force by the flapping wings.
   Total efficiency has been estimated in tethered flying fruit flies
   Drosophila by modulating their power expenditures in a virtual reality
   flight simulator while simultaneously measuring stroke kinematics,
   locomotor performance and metabolic costs. During flight, muscle
   efficiency increases with increasing flight force production, whereas
   aerodynamic efficiency of lift production decreases with increasing
   forces. As a consequence of these opposite trends, total flight
   efficiency in Drosophila remains approximately constant within the
   kinematic working range of the flight motor. Total efficiency is broadly
   independent of different profile power estimates and typically amounts
   to 2-3\%. The animal achieves maximum total efficiency near hovering
   Right conditions, when the beating wings produce flight forces that are
   equal to the body weight of the insect. It remains uncertain whether
   this small advantage in total efficiency during hovering flight was
   shaped by evolutionary factors or results from functional constraints on
   both the production of mechanical power by the indirect flight muscles
   and the unsteady aerodynamic mechanisms in flapping flight. (C) 2001
   Elsevier Science Inc. Sdl rights reserved.}},
DOI = {{10.1016/S1095-6433(01)00467-6}},
ISSN = {{1095-6433}},
Unique-ID = {{ISI:000172711300008}},
}

@article{ ISI:000172874300020,
