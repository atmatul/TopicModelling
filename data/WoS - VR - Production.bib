@inproceedings{ ISI:000279323900024,
Author = {Kuka, Daniela and Elias, Oliver and Martins, Ronald and Lindinger,
   Christopher and Pramboeck, Andreas and Jalsovec, Andreas and Maresch,
   Pascal and Hoertner, Horst and Brandl, Peter},
