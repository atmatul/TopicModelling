@inproceedings{ ISI:000323728400008,
Author = {Puerzel, Franziska and Lorenz, Mario and Wittstock, Eckhart and
   Wittstock, Volker and Neugebauer, Reimund},
Editor = {{Makris, S}},
Title = {{Applications of a Modular Interaction Framework for Virtual Reality
   Testing in a Smart Environment}},
Booktitle = {{2ND CIRP GLOBAL WEB CONFERENCE - BEYOND MODERN MANUFACTURING: TECHNOLOGY
   FOR THE FACTORIES OF THE FUTURE (CIRPE2013)}},
Series = {{Procedia CIRP}},
