@article{ ISI:000087215600011,
Author = {Campbell, DA},
Title = {{Architectural construction documents on the web: VRML as a case study}},
Journal = {{AUTOMATION IN CONSTRUCTION}},
Year = {{2000}},
Volume = {{9}},
Number = {{1}},
Pages = {{129-138}},
Month = {{JAN}},
Note = {{Annual Conference of the
   Association-for-Computer-Aided-Design-in-Architecture, QUEBEC CITY,
   CANADA, OCT, 1998}},
Organization = {{Assoc Comp Aided Design Architecture}},
Abstract = {{The Virtual Reality Modeling Language (VRML) and the World Wide Web
   (WWW) offer new opportunities to communicate an architect's design
   intent throughout the design process. We have investigated the use of
   VRML in the production and communication of construction documents, the
   final phase of architectural building design. A prototype, experimental
   Web site was set up and used to disseminate design data as VRML models
   and HTML text to the design client, contractor, and fabricators.
   In this paper, we discuss the way our construction documents were
   developed in VRML, the issues we faced implementing it, and critical
   feedback from the users of the Web space/site. We analyze the usefulness
   of VRML as a communication tool for the design and construction
   industries. Finally, we discuss technical, social, and legal issues the
   AEC industry faces as it shifts to embrace widespread use of a
   ``paperless{''} Web-based communications infrastructure for design
   documentation.}},
DOI = {{10.1016/S0926-5805(99)00055-2}},
ISSN = {{0926-5805}},
Unique-ID = {{ISI:000087215600011}},
}

@inproceedings{ ISI:000167704300024,
Author = {Damiris, N and Wild, H},
Editor = {{Rasmussen, LB and Beardon, C and Munari, S}},
Title = {{Concerning the virtual in the real}},
Booktitle = {{COMPUTERS AND NETWORKS IN THE AGE OF GLOBALIZATION}},
Series = {{INTERNATIONAL FEDERATION FOR INFORMATION PROCESSING}},
Year = {{2000}},
Volume = {{57}},
Pages = {{335-344}},
Note = {{5th World Conference on Human Choice and Computers, GENEVA, SWITZERLAND,
   AUG 25-28, 1998}},
Organization = {{Int Federat Informat Processing, TC9}},
Abstract = {{Global communication technologies need to go beyond intersubjective
   symbolic exchange toward supporting the engagement of people in their
   cultural context. Hence, what is needed is an understanding for the way
   in which the efficacy of cultural contexts manifests itself.
   Mass media have been successful in including people in a cycle of
   cultural production and consumption, but this process is accompanied by
   increased standardisation. The interindividual forms of communication,
   on the other hand, lend themselves to customisation but are too
   text-based to leverage the cultural context for purposes of creative
   collaboration.
   Virtual reality, we claim, by propagating a form of interaction based on
   immersion, could become the means of addressing and operating with
   cultural contexts. But then it needs to comprehend their ontological
   status in a new way.
   We place the efficacy of the cultural in the domain of the virtual. Our
   short paper gradually develops and refines a notion of virtuality that
   will stand up to the demands of grounding a development in that
   direction. In the process we draw on philosophy, cultural and media
   studies, and psychoanalysis.}},
ISSN = {{1571-5736}},
ISBN = {{0-7923-7253-0}},
Unique-ID = {{ISI:000167704300024}},
}

@inproceedings{ ISI:000088270100098,
Author = {Alpini, D and Pugnetti, L and Mendozzi, L and Barbieri, E and Cesarani,
   A},
Editor = {{Claussen, CF and Haid, CT and Hofferberth, B}},
Title = {{The evaluation of navigation and spatial orientation}},
Booktitle = {{EQUILIBRIUM RESEARCH, CLINICAL EQUILIBRIOMETRY AND MODERN TREATMENT}},
Series = {{INTERNATIONAL CONGRESS SERIES}},
Year = {{2000}},
Volume = {{1201}},
Pages = {{317-326}},
Note = {{20th Regular Meeting of the Barany-Society, WURZBURG, GERMANY, SEP
   12-15, 1998}},
Organization = {{Barany Soc}},
Abstract = {{Vertigo and dizziness are very common symptoms caused by an impairment
   of the vestibular system that controls equilibrium. Human equilibrium is
   a complex sensory, cognitive and motor function based on the integration
   of multi-sensory cues and aimed at the production of complex motor
   behaviour. The latter can be considered as the result of three
   integrated components:
   exploration of the environment using visual information (scanning)
   maintenance of a desired body position against the force of gravity
   (stance)
   movement into the environment against the force of gravity (walking,
   running, jumping, ...)
   The role of the vestibular system is to integrate multisensorial cues
   and to provide a continuous representation ol. the body shape (segmental
   body orientation, e.g. perception of the reciprocal position of body
   segments) and of the orientation of the body into the environment
   (spatial orientation). The knowledge of both instantaneous body shape
   and body position into the environment are the basis for the production
   of a correct motor behaviour. Thus orientation, and especially spatial
   orientation, can be considered one of the most important aspects of the
   vestibular function that influence the navigation of the environment.
   In fact, the exploration or. a natural environment carl be conceived as
   essentially an oculo-motor affair in which the head and the body help
   overcoming the constraint of a narrow field of view. Our abilities to
   localise objects in space, orient relative to them, move toward or away
   from them, reach and manipulate them,;lll depend critically on
   receiving, processing and integrating spatial information from gravity
   the visual field, and our own body. The vestibular system provides the
   integration that serves to maintain a stable map between spatial
   localization, spatial orientation, and physical space. While
   vestibulo-oculomotor and vestibulo-spinal functions are usually
   investigated by means of electronystagmography and stabilometry,
   environmental exploration and navigation cannot be easily studied in the
   laboratory. We propose that virtual reality (VR) can provide a solution,
   particularly for the assessment of vestibular influence over spatial
   cognitive activities such as memory. We describe experiments carried out
   using virtual environments aimed to assess pre-planned motor behaviours.
   An equipment designed to specifically investigate navigation is also
   reported. We also propose a simple test to be performed in a real
   environment designed to assess and document spatial orientation and
   spatial memory.}},
ISSN = {{0531-5131}},
ISBN = {{0-444-50012-X}},
Unique-ID = {{ISI:000088270100098}},
}

@inproceedings{ ISI:000089526600011,
