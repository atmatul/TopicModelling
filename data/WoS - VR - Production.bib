@inproceedings{ ISI:000274641800116,
Author = {Pamintas, Eugen and Banciu, Felicia Veronica},
Editor = {{Nedelcu, D and Slatineanu, L and Mazuru, S}},
Title = {{INTEGRATION OF CONTROL AND MAINTENANCE IN MACHINING PROCESS AS AN
   INTERMEDIARY STEP}},
Booktitle = {{PROCEEDINGS OF THE 13TH INTERNATIONAL CONFERENCE MODERN TECHNOLOGIES,
   QUALITY AND INNOVATION: MODTECH 2009 - NEW FACE OF TMCR}},
Series = {{Proceedings of the International Conference ModTech}},
Year = {{2009}},
