@inproceedings{ ISI:000180358500151,
Author = {Kibira, D and McLean, C},
Editor = {{Yucesan, E and Chen, CH and Snowdon, JL and Charnes, JM}},
Title = {{Virtual reality simulation of a mechanical assembly production line}},
Booktitle = {{PROCEEDINGS OF THE 2002 WINTER SIMULATION CONFERENCE, VOLS 1 AND 2}},
Year = {{2002}},
