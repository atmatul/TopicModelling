@inproceedings{ ISI:000186578600474,
Author = {Lee, MY and Chang, CC and Lin, CC and Lo, LJ and Chen, YR},
Book-Group-Author = {{IEEE
   IEEE}},
Title = {{T medical rapid prototyping in custom implant design for craniofacial
   reconstruction}},
Booktitle = {{2003 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN AND CYBERNETICS, VOLS
   1-5, CONFERENCE PROCEEDINGS}},
Series = {{IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN, AND CYBERNETICS,
   CONFERENCE PROCEEDINGS}},
Year = {{2003}},
Pages = {{2903-2908}},
Note = {{IEEE International Conference on Systems, Man and Cybernetics (SMC 03),
   WASHINGTON, D.C., OCT 05-08, 2003}},
Organization = {{IEEE, Syst, Man \& Cybernet Soc}},
Abstract = {{Trauma, cranial tumors, infected craniotomy bone flaps and external
   neurosurgical decompression are the main causes for large cranial
   defects. The success of the reconstructive craniofacial surgery depends
   on good preoperative evaluation of the deformity, forma lion of the
   surgical planning, preparation of precise-fitting implants, and accurate
   execution of the operation. Recently, reversing engineering and rapid
   prototyping technologies have been applied to produce physical models in
   various medical applications. In this article, the applications of
   3-dimensional medical image reconstruction and rapid prototyping
   techniques were used for inplant design and production of implant for
   patient with large crania I defect reconstruction. The medical images
   were manipulated and surgical planning was performed in virtual
   environment by moving the image objects. The customized implant was
   prefabricated based on the 3-dimensional image created to cover the
   cranial defect. A skull model is also produced for real touch
   evaluation, surgical planning and rehearsal of surgical procedures. The
   results show that both of 3-dimensional medical image model and physical
   skull model were helpful for planning of craniofacial surgery and design
   of customized inplant. The surgery result was satisfactory without
   complication. Particular advantage was the unlimited trials in surgical
   simulation for using imaging model, and the close-to-reality surgical
   rehearsal for using physical model.}},
ISSN = {{1062-922X}},
ISBN = {{0-7803-7952-7}},
Unique-ID = {{ISI:000186578600474}},
}

@article{ ISI:000182072800011,
Author = {Chan, DSK},
Title = {{Simulation modelling in virtual manufacturing analysis for integrated
   product and process design}},
Journal = {{ASSEMBLY AUTOMATION}},
Year = {{2003}},
Volume = {{23}},
Number = {{1}},
Pages = {{69-74}},
Abstract = {{This paper presents the work on the implementation of developing
   graphical simulation models. The work of development emphasises the
   integrated approach in the product and process design, where same
