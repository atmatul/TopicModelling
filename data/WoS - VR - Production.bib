Year = {{2002}},
Pages = {{228-233}},
Note = {{6th China-Japan International Conference on Industrial Management, XIAN,
   PEOPLES R CHINA, SEP 16-18, 2002}},
Abstract = {{in this research, design system by Virtud Reality (referred to as VR
   limaller) for assembly process is developed to shorten the period from
   design to quantity production. In this system, 3-dimensional magnetic
   sensors are attached to a worker for measuring the working posture. In
   VR, layout and size of tools, parts shelves and work desks are flexibly
   set up, and full-scale work area can be realized. Workers are able to
   acquire die working procedure by displaying pictures of parts and
   locations sequentially in virtual space according to die progress of
   operation.
   Work condition is assessed by die measured working posture and
   interference among workers on the principle of ``Motion Economy{''}.
   Moreover advices of workers can be reflected to the process design. This
   system realizes above-mentioned all functions and allows us to
   accomplish the most appropriate production system in a short time.}},
ISBN = {{7-80183-018-0}},
Unique-ID = {{ISI:000179334800036}},
}

@inproceedings{ ISI:000175725500035,
Author = {Park, CH and Ko, H and Kim, IJ and Ahn, SC and Kwon, YM and Kim, HG},
